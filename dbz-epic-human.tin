#EVENT {SESSION CONNECTED}
{
  #VAR {session[name]} {%0};
};

#session {dbzepic} {dbzepic.org} {6200}

#config {command echo} {off};
#config {repeat enter} on

#format {session[ScreenRows]} {%R};
#format {session[ScreenCols]} {%C};

#var move_speed 5

#NOP === DEFAULTS ===
#READ dbz-epic-defaults.tin;

#NOP === SPECIAL MELEE ==
#alias wo {wolf}

#NOP == DEFENSE ==
#alias so {solar}

#NOP === BLASTS ===
#alias do {dodonpa}
#alias tr {tri}
#alias dd {destructo}
